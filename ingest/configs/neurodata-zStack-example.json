{
  "schema": {
      "name": "neurodata-schema",
      "validator": "NeurodataValidator"
  },
  "client": {
    "backend": {
      "name": "neurodata",
      "class": "NeurodataBackend",
      "host": "localhost:8000",
      "protocol": "https"
    },
    "path_processor": {
      "class": "ingest.plugins.stack.ZindexStackPathProcessor",
      "params": {
        "root_dir": "",
        "extension": "png",
        "base_filename": "<o:3253>_my_stack_section<p:3>"
      }
    },
    "tile_processor": {
      "class": "ingest.plugins.stack.ZindexStackTileProcessor",
      "params": {
        "filesystem": "local",
        "extension": "png"
      }
    }
  },
  "database": {
    "collection": "my_col_1",
    "experiment": "my_exp_1",
    "channel": "my_ch_1"
  },
  "ingest_job": {
    "resolution": 0,
    "extent": {
      "x": [0, 8192],
      "y": [0, 8192],
      "z": [0, 500],
      "t": [0, 1]
    },
    "tile_size": {
      "x": 1024,
      "y": 1024,
      "z": 1,
      "t": 1
    }
  }
}
